<robot name="wam"
  xmlns:xacro="http://www.ros.org/wiki/xacro">

  <!-- Includea all the WAM stuff -->
  <xacro:include filename="$(find barrett_model)/models/wam_top.urdf.xacro"/>

  <!-- Root link is in the world frame-->
  <link name="$(arg parent_link)"/>

  <!-- BHand -->
  <xacro:bhand prefix="$(arg prefix)/bhand" parent_link="$(arg parent_link)" xyz="0 0 0" rpy="0 0 0" primitive="$(arg primitive)"/>
</robot>
